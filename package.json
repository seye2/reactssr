{
  "name": "reactssr",
  "version": "0.0.1",
  "description": "reactssr",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:client": "rm -rf ./dev/client/* && webpack --config ./webpack/webpack.config.dev.client.js",
    "dev:server": "rm -rf ./dev/server/* && webpack --config ./webpack/webpack.config.dev.server.js",
    "prod-test:client": "rm -rf ./functions/reactssr/client/* && webpack --config ./webpack/webpack.config.prod.client.js --env.ENV=test",
    "prod:client": "rm -rf ./functions/reactssr/client/* && webpack --config ./webpack/webpack.config.prod.client.js --env.ENV=deploy",
    "prod-test:server": "rm -rf ./functions/reactssr/server/* && webpack --config ./webpack/webpack.config.prod.server.js --env.ENV=test",
    "prod:server": "rm -rf  ./functions/reactssr/server/* && webpack --config ./webpack/webpack.config.prod.server.js --env.ENV=deploy",
    "dev": "export NODE_ENV=development && npm-run-all --parallel dev:client dev:server && node ./dev/server/local_server.js",
    "prod-test": "export NODE_ENV=production && export NODE_STATUS=test && npm-run-all --parallel prod-test:server prod:client && node ./functions/reactssr/server/index.js",
    "prod": "export NODE_ENV=production && export NODE_STATUS=deploy && npm-run-all --parallel prod:server prod:client && npm run deploy",
    "deploy": "./deploy.sh"
  },
  "dependencies": {
    "aws-serverless-express": "^3.1.3",
    "body-parser": "^1.18.2",
